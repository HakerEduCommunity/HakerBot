{
    "extends": "airbnb",
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "linebreak-style": 0,
        "no-console": 0,
        "consistent-return": 0,
        "no-unused-vars": 0,
        "max-len": 0,
        "no-shadow": 0,
        "no-use-before-define": 0,
        "no-inner-declarations": 0,
        "no-plusplus": 0
    }
}